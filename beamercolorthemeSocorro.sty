\mode<presentation>

\definecolor{desertblue}{HTML}{01385F} %bkgrd
\definecolor{deserttan}{HTML}{ECE2C6}

\setbeamercolor*{palette primary}{bg=desertblue,fg=deserttan}
\setbeamercolor*{palette secondary}{fg=desertblue,bg=deserttan}
\setbeamercolor{palette tertiary}{bg=desertblue,fg=white}
\setbeamercolor{palette quaternary}{bg=desertblue,fg=white}

\setbeamercolor{titlelike}{parent=palette primary}
\setbeamercolor{frametitle}{parent=palette primary}
\setbeamercolor{subframetitle}{fg=desertblue}

\setbeamercolor{title page}{parent=palette primary}
\setbeamercolor{background canvas}{bg=deserttan}

\setbeamercolor*{separation line}{}
\setbeamercolor*{fine separation line}{}

\setbeamercolor{itemize item}{fg=desertblue}

\setbeamercolor{section in head/foot}{bg=desertblue, fg=deserttan}
\setbeamercolor{subsection in head/foot}{bg=desertblue}

\mode
<all>
